{
  "name": "education-platform",
  "author": "@john-nhoj",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dependencies:check": "ncu",
    "dependencies:update": "ncu -u",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint:css": "stylelint \"./**/*.{css,ts,tsx}\"",
    "lint:fix": "eslint --max-warnings=0 --ext .ts,.tsx,.js --fix .",
    "prepush": "npm run type-check && npm run lint",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.4",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "npm-check-updates": "^11.6.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  }
}
